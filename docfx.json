{
  "build": {
    "content": [
      {
        "files": [ "analytics/**/*.md", "*.md", "toc.yml", "powershell/**/*.md", "restapi/**/*.md", "zerto/**/*.md" ]
      }
    ],
    "resource": [
      {
        "files": [ "media/**", "analytics/images/**", "powershell/images/**", "restapi/images/**", "zerto/images/**", "*.png", "*.ico"]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Zerto Docs",
      "_enableSearch": true,
      "_appLogoPath": "media/Zerto-Logo_RGB2.png"
    },
    "markdownEngineName": "markdig",
    "dest": "_site",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}
